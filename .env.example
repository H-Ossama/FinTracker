# Example environment variables for the Expo app (DO NOT put secrets here)
# Copy to .env for local development.

# Backend base URL (no trailing slash). Dev defaults will be inferred if omitted.
EXPO_PUBLIC_BACKEND_URL=http://localhost:3001

# Firebase (public identifiers; still keep real values out of git if you prefer)
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=

# Google auth / sign-in
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# Ads
ADMOB_ANDROID_APP_ID=
ADMOB_IOS_APP_ID=

# Optional
EXPO_PUBLIC_CLOUD_STORAGE_ENDPOINT=
EXPO_PUBLIC_DEV_PIN=
